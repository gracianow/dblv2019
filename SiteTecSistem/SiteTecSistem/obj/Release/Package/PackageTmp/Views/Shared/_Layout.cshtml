<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Site Tec Sistem Sistemas</title>
    @*@Styles.Render("~/Content/tecsistem/css")
        @Scripts.Render("~/bundles/modernizr")*@

    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">

    <!-- Favicons -->
    <link href="~/Content/tecsistem/img/favicon.png" rel="icon">
    <link href="~/Content/tecsistem/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,400i,600,700|Raleway:300,400,400i,500,500i,700,800,900" rel="stylesheet">

    <!-- Bootstrap CSS File -->
    <link href="~/Content/tecsistem/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Libraries CSS Files -->
    <link href="~/Content/tecsistem/lib/nivo-slider/css/nivo-slider.css" rel="stylesheet">
    <link href="~/Content/tecsistem/lib/owlcarousel/owl.carousel.css" rel="stylesheet">
    <link href="~/Content/tecsistem/lib/owlcarousel/owl.transitions.css" rel="stylesheet">
    <link href="~/Content/tecsistem/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/Content/tecsistem/lib/animate/animate.min.css" rel="stylesheet">
    <link href="~/Content/tecsistem/lib/venobox/venobox.css" rel="stylesheet">

    <!-- Nivo Slider Theme -->
    <link href="~/Content/tecsistem/css/nivo-slider-theme.css" rel="stylesheet">

    <!-- Main Stylesheet File -->
    <link href="~/Content/tecsistem/css/style.css" rel="stylesheet">

    <!-- Responsive Stylesheet File -->
    <link href="~/Content/tecsistem/css/responsive.css" rel="stylesheet">

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.mask/1.14.11/jquery.mask.min.js"></script>

    <script type="text/javascript">$("#txtFoneFixo").mask("(00) 0000-0009");</script>

    <script type="text/javascript">$("#txtCelular").mask("(00) 00000-0009");</script>

</head>
<body data-spy="scroll" data-target="#navbar-example">
    <div class="container">
        <partial name="_CookieConsentPartial" />
        <main role="main" class="pb-3">

        </main>
    </div>

    <div class="container body-content">
        @Html.Partial("_Menu")
        @RenderBody()
        <hr />
        <br />
        @Html.Partial("_Footer")
    </div>

    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)*@
</body>
</html>
