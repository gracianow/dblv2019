<!-- Start contact Area -->
<section id="contact">
    <div class="contact-area">
        <div class="contact-inner area-padding">
            <div class="contato-overly"></div>
            <div class="container ">
                <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="section-headline text-center">
                            <img src="~/Content/tecsistem/img/logo/logo_221_108.png" style="max-width:22%" /><br />
                            <h2>Contatos</h2>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <!-- Start contact icon column -->
                    <div class="col-md-4 col-sm-4 col-xs-12">
                        <div class="contact-icon text-center">
                            <div class="single-icon">
                                <i class="fa fa-mobile"></i>
                                <p>Fone: <a href="tel:+551143236944">+55 11 4323-6944</a></p>
                                <p>Celular: <a href="tel:+5511941672104">+55 11 94167-2104</a></p>
                            </div>
                        </div>
                    </div>
                    <!-- Start contact icon column -->
                    <div class="col-md-4 col-sm-4 col-xs-12">
                        <div class="contact-icon text-center">
                            <div class="single-icon">
                                <i class="fa fa-envelope-o"></i>
                                <p>
                                    <span>E-mail: </span>contato@tecsistemsistemas.com.br
                                </p>
                            </div>
                        </div>
                    </div>
                    <!-- Start contact icon column -->
                    <div class="col-md-4 col-sm-4 col-xs-12">
                        <div class="contact-icon text-center">
                            <div class="single-icon">
                                <i class="fa fa-map-marker"></i>
                                <p>
                                    Av. Raimundo Pereira Magalhães, 11.773 - Lj 1<br>
                                    <span>São Paulo - SP</span>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <!-- Start  contact -->
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        <div id="divForm" class="form contact-form">
                            <form method="post" enctype="multipart/form-data" role="form" class="">
                                <div class="form-row contactForm">
                                    <div class="form-group">
                                        <input type="text" name="nome" class="form-control" id="nome" placeholder="Nome" data-rule="minlen:4" data-msg="Please enter at least 4 chars" required />
                                        <div class="validation"></div>
                                    </div>
                                </div>
                                <div class="form-row contactForm">
                                    <div class="form-group">
                                        <input type="text" name="empresa" class="form-control" id="empresa" placeholder="Empresa" data-rule="minlen:4" data-msg="Please enter at least 4 chars" />
                                        <div class="validation"></div>
                                    </div>
                                </div>
                                <div class="form-row contactForm">
                                    <div class="form-group">
                                        <input type="email" class="form-control" name="email" id="email" placeholder="E-mail" data-rule="email" data-msg="Insira um e-mail válido!" required />
                                        <div class="validation"></div>
                                    </div>
                                </div>
                                <div class="form-row contactForm">
                                    <div class="form-group col-md-6">
                                        <input type="tel" name="celular" id="txtCelular" pattern="\([0-9]{2}\)[\s][0-9]{5}-[0-9]{4,4}"
                                               class="form-control" placeholder="Celular" data-rule="minlen:11"
                                               data-msg="Digite pelo menos 11 caracteres!" required />
                                        <div class="validation"></div>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <input type="tel" name="fonefixo" id="txtFoneFixo" pattern="\([0-9]{2}\)[\s][0-9]{4}-[0-9]{4,5}"
                                               class="form-control" placeholder="Fone Fixo" />
                                        <div class="validation"></div>
                                    </div>
                                </div>
                                <div class="form-row contactForm">
                                    <div class="form-group">
                                        <input type="text" class="form-control" name="assunto" id="subject" placeholder="Assunto" data-rule="minlen:4" data-msg="Escreva pelo menos 4 caracteres!" required />
                                        <div class="validation"></div>
                                    </div>
                                    <div class="form-group">
                                        <textarea class="form-control" name="mensagem" id="taMensagem" rows="5" required>
                                    </textarea>
                                        <div class="validation"></div>
                                    </div>
                                    <div class="form-group text-center">
                                        <button type="submit" data-toggle="modal" data-target="#modalMessage">Enviar</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>

                    <!-- Modal -->
                    @if (TempData["mailMsg"] != null)
                    {
                        Html.RenderPartial("_ModalEnviarEmail", TempData["mailMsg"]);
                    }
                    <!-- end modal-->
                    <!-- End Left contact -->
                    <!-- Start Google Map -->
                    <div class="col-md-6 col-sm-6 col-xs-12" style="text-align:center !important; vertical-align:middle !important">
                        <div class="card mb-6" style="background-color:black; border-radius:3px;">
                            <img class="card-img-top" src="~/Content/tecsistem/img/about/tecsis1000x550_seg1.jpg" style="border-radius:3px;" />
                            <div class="card-body">
                                <h3 class="card-title" style="color:white !important;">Otimize sua segurança!</h3>
                                <br />
                                <h5 class="card-text text-center" style="color:white !important;">
                                    <strong>
                                        Controle de Acesso<br />
                                        CFTV HD<br />
                                        Detecção<br />
                                        Alarmes de Incêndio
                                    </strong>
                                </h5>
                                <br />
                                @*<p class="card-text"><small class="text-muted">Atualizados 3 minutos atrás</small></p>*@
                            </div>
                        </div>
                    </div>
                    <!-- End Google Map -->
                </div>
            </div>
        </div>
    </div>
</section>
