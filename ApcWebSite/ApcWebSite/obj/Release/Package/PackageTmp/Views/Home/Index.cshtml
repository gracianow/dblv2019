            <form method="post" enctype="multipart/form-data" role="form" class="contactForm">
                <div class="form-row text-center">
                    <div class="form-group">
                        <div class="text-center">
                            <h5 style="color:blue;">@ViewBag.Mensagem</h5>
                            <h5 style="color:red;">@ViewBag.Error</h5>
                        </div>
                    </div>
                </div>
                <div class="form-row text-center">
                    <div class="form-group col-md-6">
                        <input type="text" name="nome" class="form-control" id="nome" placeholder="Nome" data-rule="minlen:4" data-msg="Digite pelo menos 4 caracteres!" required />
                        <div class="validation"></div>
                    </div>
                    <div class="form-group col-md-6">
                        <input type="email" class="form-control" name="email" id="email" placeholder="E-mail" data-rule="email" data-msg="Por favor insira um e-maiil válido!" required />
                        <div class="validation"></div>
                    </div>
                </div>
                <div class="form-row text-center">
                    <div class="form-group col-md-6">
                        <input type="text" name="empresa" class="form-control" id="empresa" placeholder="Empresa" data-rule="minlen:4" data-msg="Digite pelo menos 4 caracteres!" />
                        <div class="validation"></div>
                    </div>
                    <div class="form-group col-md-3">
                        <input type="tel" name="celular" id="txtCelular" pattern="\([0-9]{2}\)[\s][0-9]{5}-[0-9]{4,4}"
                               class="form-control" placeholder="Celular" data-rule="minlen:11"
                               data-msg="Digite pelo menos 11 caracteres!" required />
                        <div class="validation"></div>
                    </div>
                    <div class="form-group col-md-3">
                        <input type="tel" name="fonefixo" id="txtFoneFixo" pattern="\([0-9]{2}\)[\s][0-9]{4}-[0-9]{4,5}"
                               class="form-control" placeholder="Fone Fixo" data-rule="minlen:10"
                               data-msg="Digite pelo menos 10 caracteres!" required />
                        <div class="validation"></div>
                    </div>
                </div>
                <div class="form-row text-center">
                    <div class="form-group col-md-6">
                        <select name="tipo" class="form-control" id="tipo">
                            <option value="Cotação">Cotação</option>
                            @*<option value="Atendimento">Atendimento</option>*@
                        </select>
                    </div>
                    <div class="form-group col-md-6">
                        <input type="text" class="form-control" name="assunto" id="assunto" placeholder="Assunto" data-rule="minlen:4" data-msg="Por favor digite o assunto!" required />
                        <div class="validation"></div>
                    </div>

                </div>
                <div class="form-row text-center">
                    <div class="form-group col-md-12">
                        <textarea class="form-control" name="mensagem" rows="5" data-rule="required" data-msg="Digite sua mensagem!" placeholder="Comentários" required></textarea>
                        <div class="validation"></div>
                    </div>
                </div>
                <div class="form-row text-center">
                    <div class="form-group col-md-12">
                        <button id="enviar" class="btn btn-primary" type="submit">Enviar</button>
                    </div>
                </div>
            </form>
        </div>
    </section><!-- #contact -->

</main>

