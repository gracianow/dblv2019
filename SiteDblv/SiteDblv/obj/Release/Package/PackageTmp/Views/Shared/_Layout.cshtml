<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Doblevê Soluções em T.I. &amp; Negócios</title>

    @Styles.Render("~/Content/assets/css/bootstrap.css",
    "~/Content/assets/css/bootstrap-responsive.css", "~/Content/assets/css/docs.css",
    "~/Content/assets/css/prettyPhoto.css", "~/Content/assets/js/google-code-prettify/prettify.css",
    "~/Content/assets/css/flexslider.css", "~/Content/assets/css/refineslide.css", "~/Content/assets/css/font-awesome.css",
    "~/Content/assets/css/animate.css", "~/Content/assets/css/style.css", "~/Content/assets/color/default.css",
    "~/Content/assets/css/dblv.css")


    <environment include="Development">
        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.2.0/css/all.css" />
        <link rel="shortcut icon" href="~/Content/assets/ico/favicon.ico" />
        <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/Content/assets/ico/apple-touch-icon-144-precomposed.png" />
        <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/Content/assets/ico/apple-touch-icon-114-precomposed.png" />
        <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/Content/assets/ico/apple-touch-icon-72-precomposed.png" />
        <link rel="apple-touch-icon-precomposed" href="~/Content/assets/ico/apple-touch-icon-57-precomposed.png" />

    </environment>

    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/cookieconsent@3/build/cookieconsent.min.css" />

</head>
<body>
    <div id="wrapper">

        <div class="container">
            @RenderPage("~/Views/Shared/_Menu.cshtml")
        </div>

        <div class="container">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>

        <div class="container">
            @RenderPage("~/Views/Shared/_Footer.cshtml")
        </div>
    </div>

    @Scripts.Render("https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.js")
    @Scripts.Render("https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.1/jquery.validate.js")
    @Scripts.Render("https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.bundle.min.js")
    @Scripts.Render("https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.js")
    @Scripts.Render("~/Content/assets/js/google-code-prettify/prettify.js")
    @Scripts.Render("~/Content/assets/js/jquery.prettyPhoto.js")
    @Scripts.Render("~/Content/assets/js/portfolio/jquery.quicksand.js")
    @Scripts.Render("~/Content/assets/js/portfolio/setting.js")
    @Scripts.Render("~/Content/assets/js/hover/jquery-hover-effect.js")
    @Scripts.Render("~/Content/assets/js/jquery.flexslider.js")
    @Scripts.Render("~/Content/assets/js/classie.js")
    @Scripts.Render("~/Content/assets/js/cbpAnimatedHeader.min.js")
    @Scripts.Render("~/Content/assets/js/jquery.refineslide.js")
    @Scripts.Render("~/Content/assets/js/jquery.ui.totop.js")
    @Scripts.Render("~/Content/assets/js/custom.js")
    @Scripts.Render("~/Content/js/site.js")

    @RenderSection("scripts", required: false)

</body>
</html>
