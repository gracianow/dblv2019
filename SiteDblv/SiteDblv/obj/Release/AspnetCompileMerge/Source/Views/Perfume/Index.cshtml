    <section id="contact" class="section-padding">
        <div class="container">
            <div class="row">
                <div class="col-md-12 text-center">
                    <h1 class="header-h">Faça o seu pedido</h1>
                    <p class="header-p">
                        Faça seu pedido ou peça mais informações. Nós queremos te ouvir!
                    </p>
                </div>
            </div>
            <div class="row msg-row">
                <div class="col-md-4 col-sm-4 mr-15">
                    <div>
                        <img src="~/Content/perfumes/logos/logo_claudia_perfumes_240x100.png" style="max-width:85%"/>
                    </div>
                    <div class="media-2">
                        <div class="media-left">
                            
                            <div class="contact-phone bg-1 text-center"><span class="phone-in-talk fa fa-phone"></span></div>
                        </div>
                        <div class="media-body">
                            <h4 class="dark-blue regular">Fone & WhatsApp</h4>
                            <p class="light-blue regular alt-p">+55 11 979840905 - <span class="contacts-sp">Claudia</span></p>
                        </div>
                    </div>
                    <div class="media-2">
                        <div class="media-left">
                            <div class="contact-email bg-14 text-center"><span class="hour-icon fa fa-clock-o"></span></div>
                        </div>
                        <div class="media-body">
                            <h4 class="dark-blue regular">Sempre on line</h4>
                            @*<p class="light-blue regular alt-p"> Monday to Friday 09.00 - 24:00</p>
                    <p class="light-blue regular alt-p">
                        Friday and Sunday 08:00 - 03.00
                    </p>*@
                        </div>
                    </div>
                </div>
                <div class="col-md-8 col-sm-8">
                    <form action="" method="post" role="form" class="contactForm">

                        <div class="col-md-12 col-sm-12 contact-form pad-form">
                            <div class="form-group label-floating is-empty">
                                <input type="text" name="nome" class="form-control" id="txtNome" placeholder="Nome" data-rule="minlen:4" data-msg="Digite pelo menos 4 caracteres!" required />
                                <div class="validation"></div>
                            </div>
                        </div>
                        <div class="col-md-6 col-sm-6 contact-form">
                            <div class="form-group">
                                <input type="tel" class="form-control label-floating is-empty" name="celular" id="txtCelular" placeholder="Celular" data-rule="required" data-msg="Este campo é obrigatório!" required />
                                <div class="validation"></div>
                            </div>
                        </div>
                        <div class="col-md-6 col-sm-6 contact-form pad-form">
                            <div class="form-group">
                                <input type="email" class="form-control label-floating is-empty" name="email" id="txtEmail" placeholder="E-mail" data-rule="email" data-msg="Digite um e-mail válido!" required />
                                <div class="validation"></div>
                            </div>
                        </div>

                        <div class="col-md-12 contact-form pad-form">
                            <div class="form-group label-floating is-empty">
                                <input type="text" id="txtAssunto" class="form-control" name="assunto" required data-msg="Digite um assunto" placeholder="Assunto"/>
                                <div class="validation"></div>
                            </div>
                        </div>
                        <div class="col-md-12 contact-form">
                            <div class="form-group label-floating is-empty">
                                <textarea id="taMensagem" class="form-control" name="mensagem" rows="5" required data-msg="Seu comentários e pedidos" placeholder="Mensagem"></textarea>
                                <div class="validation"></div>
                            </div>
                        </div>
                        <div class="col-md-12 btnpad">
                            <div class="contacts-btn-pad">
                                <button id="btEnviar" class="contacts-btn text-white" type="submit" data-toggle="modal" data-target="#modalMessage">Enviar</button>
                            </div>
                        </div>
                    </form>
                </div>
                <!-- Modal -->
                @if (TempData["mailMsg"] != null)
                {
                    Html.RenderPartial("_ModalEnviarEmail", TempData["mailMsg"]);
                }
                <!-- end modal-->
            </div>
            </div>
        </div>
    </section>
